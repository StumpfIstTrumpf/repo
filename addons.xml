<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon
	id="plugin.video.streamzto"
	version="0.3.0"
	name="Stream4k.net"
	provider-name="stumpf"
	>
	<requires>
		<import addon="xbmc.addon" version="12.0.0"/>
        <import addon="xbmc.json" version="6.0.0"/>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.simplejson" version="3.3.0"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="streamz.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
	    <language>de</language>
		<summary>Nette Grüße von Stream4k.net</summary>
		<description>Stream4k.net - Mit diesem Addon kannst du ohne Probleme die Streams von Stream4k.net angucken, völlig unkopliziert via XBMC.</description>
		<platform>all</platform>
	</extension>
</addon>

<addon
	id="script.streamztv"
	version="0.0.9"
	name="Stream4k.net TV"
	provider-name="stumpf"
	>
	<requires>
		<import addon="xbmc.addon" version="12.0.0"/>
        <import addon="xbmc.json" version="6.0.0"/>
        <import addon="xbmc.python" version="2.1.0"/>
        <import addon="script.module.simplejson" version="3.3.0"/>
	</requires>
	<extension point="xbmc.python.script" library="streamz.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
	    <language>de</language>
		<summary>Das Neuste Plugin von Stream4k.net</summary>
		<description>Stream4k.net TV - Diese Addon soll dir das Gefühl eines Fernsehers geben.</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
